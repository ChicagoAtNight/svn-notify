#!/usr/bin/perl -w

eval 'exec /usr/bin/perl -w -S $0 ${1+"$@"}'
    if 0; # not running under some shell

use strict;
use FindBin;
use File::Spec::Functions;

my $command = shift;
my $rev = pop;

my $file = catfile($FindBin::Bin, updir, 'data', $command, "$rev.txt");

open F, "<$file" or die "Cannot open '$file': $!\n";
print for <F>;
close F;
